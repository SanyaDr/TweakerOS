<Application x:Class="TweakerOS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="./View/Windows/MainWindow.xaml"
             
             >
    <Application.Resources>
         <ResourceDictionary>
             <ResourceDictionary.MergedDictionaries>
                 <!-- Icons -->
                 <ResourceDictionary Source="/Resources/Icons/IconsSvg.xaml"/>

                 <!-- Themes -->
                 <ResourceDictionary Source="/Resources/Themes/LightTheme.xaml"/>
                 
                 <!-- Styles -->
                 <ResourceDictionary Source="/Resources/MainDesign.xaml"/>
                 <ResourceDictionary Source="/Resources/TweaksDesign.xaml"/>
                 
             </ResourceDictionary.MergedDictionaries>
         </ResourceDictionary>
    </Application.Resources>
</Application>
